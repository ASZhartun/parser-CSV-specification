<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service.Director">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Расчет спецификации 1.0" textAlignment="CENTER">
            <font>
                <Font name="ISOCTEUR" size="24.0" />
            </font>
            <VBox.margin>
                <Insets top="25.0" />
            </VBox.margin>
        </Text>
        <VBox alignment="CENTER">
            <children>
                <Label text="путь к файлу">
                    <font>
                        <Font name="ISOCTEUR" size="18.0" />
                    </font>
                </Label>
                <HBox alignment="CENTER" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" text="Файл">
                            <HBox.margin>
                                <Insets left="25.0" />
                            </HBox.margin>
                            <font>
                                <Font name="ISOCTEUR" size="14.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" text="Расчет">
                            <HBox.margin>
                                <Insets left="25.0" />
                            </HBox.margin>
                            <font>
                                <Font name="ISOCTEUR" size="14.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" text="Результаты">
                            <font>
                                <Font name="ISOCTEUR" size="14.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="25.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="25.0" top="10.0" />
                    </padding>
                </HBox>
                <Pane>
                    <VBox.margin>
                        <Insets left="50.0" right="50.0" />
                    </VBox.margin>
                </Pane>
            </children>
            <VBox.margin>
                <Insets top="15.0" />
            </VBox.margin>
        </VBox>
        <VBox alignment="TOP_CENTER" prefHeight="289.0" prefWidth="480.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Дополнительные КЖ.И">
                    <font>
                        <Font name="ISOCTEUR" size="24.0" />
                    </font>
                    <VBox.margin>
                        <Insets top="15.0" />
                    </VBox.margin>
                </Text>
                <TreeTableView fx:id="lib_storage_table">
                    <columns>
                        <TreeTableColumn fx:id="lib_storage_table_names" minWidth="25.0" prefWidth="75.0" text="Имя КЖ.И" />
                        <TreeTableColumn prefWidth="75.0" text="Масса КЖ.И" />
                        <TreeTableColumn prefWidth="75.0" text="Путь" />
                    </columns>
                    <columnResizePolicy>
                        <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                    </VBox.margin>
                </TreeTableView>
                <HBox fx:id="lib_storage_actions" alignment="CENTER">
                    <children>
                        <Button fx:id="addRPCButton" mnemonicParsing="false" onAction="#addRPC" text="Добавить КЖ.И">
                            <HBox.margin>
                                <Insets left="25.0" right="25.0" />
                            </HBox.margin>
                            <font>
                                <Font name="ISOCTEUR" size="14.0" />
                            </font>
                        </Button>
                        <Button fx:id="deleteRPCButton" mnemonicParsing="false" onAction="#deleteRPC" text="Удалить КЖ.И">
                            <HBox.margin>
                                <Insets left="25.0" right="25.0" />
                            </HBox.margin>
                            <font>
                                <Font name="ISOCTEUR" size="14.0" />
                            </font>
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="25.0" left="25.0" right="25.0" top="5.0" />
                    </padding>
                </HBox>
            </children>
        </VBox>
        <VBox alignment="CENTER">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="as.zhartun@gmail.com, 2022" textAlignment="CENTER">
                    <font>
                        <Font name="ISOCTEUR Italic" size="12.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </Text>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </VBox>
    </children>
</VBox>
